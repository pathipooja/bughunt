#include<stdio.h>
int* n_time_array(int arr[],int n)
{
    int new_array[5]={0};
    int i;
    for(i=0;i<5;i++)
    {
        new_array[i]=arr[i]*n;
    }
    return *new_array;
}
int main()
{
    int a[5]={1,2,3,4,5},i;
    int res[5];
 *res= n_time_array(a,5);
    for(i=0;i<5;i++)
    {

        printf("%d ",*res);
    }
    return 0;
}
